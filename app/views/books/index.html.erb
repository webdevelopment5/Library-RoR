<h1>Listing books</h1>

<table>
  <thead>
    <tr>
      <th>Isbn</th>
      <th>Author</th>
      <th>Title</th>
      <th>Physical</th>
      <th>Electronic</th>
      <th>Loandate</th>
      <th>Returndate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.ISBN %></td>
        <td><%= book.Author %></td>
        <td><%= book.Title %></td>
        <td><%= book.physical %></td>
        <td><%= book.electronic %></td>
        <td><%= book.loanDate %></td>
        <td><%= book.returnDate %></td>
        <td><%= link_to 'Show', book %></td>
        <td><%= link_to 'Edit', edit_book_path(book) %></td>
        <td><%= link_to 'Destroy', book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book', new_book_path %>
